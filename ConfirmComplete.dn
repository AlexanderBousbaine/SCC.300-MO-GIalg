component provides ConfirmComplete requires data.IntUtil iu, io.File{
	
	//should take in two parameters, one being the number of the completed run and the other being the path to save completion file at.
	void ConfirmComplete:generateCompletionFile(int run_number, char path[])
	{	
		File tracker = new File("$path/$(run_number)", File.CREATE)
		tracker.close()
	}
}